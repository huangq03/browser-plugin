{
    "manifest_version": 3,
    "name": "URL Decoder",
    "version": "1.0",
    "description": "Decode selected text using URL decode.",
    "permissions": ["contextMenus", "activeTab"],
    "action": {
        "default_popup": "url_decode_popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}